<ion-view title="{{resTitle}}">
    <ion-nav-buttons side="left">
        <button ng-click="back(-1)" class="button button-clear">
            <i class="ion-ios-arrow-back"></i>
            <span id="barItemlText">返 回</span>
        </button>
    </ion-nav-buttons>

    <ion-nav-buttons side="right">
        <button class="button button-clear button-light icon-large" ng-click="changeStyle()">
            <span id="barItemrText" ng-bind="showType === false ? '大 图' : '列 表'"></span>
            <i ng-class="showType === false ? 'ion-images' : 'ion-navicon'"></i>
        </button>
    </ion-nav-buttons>
    <ion-content>
        <div id="mainContent">
        <div class="list margin-bottom-none" ng-if="showType === false">
            <div class="item item-thumbnail-left floorMain" ng-click="resItemClick($index)" ng-repeat="resList in proResList track by $index">
                <img id="prListImg" _lazysrc="{{resList.proListImg}}" onerror=this.src=errImg>
                <p class="proName" ng-bind="resList.proName"></p>
                <p class="margin-bottom-none"><span class="proModel" ng-bind="resList.proModel"></span></p>
                <p><span class="proPrice" ng-bind="resList.proPrice"></span></p>
            </div>
        </div>
        </div>

        <div class="row" ng-if="showType === true">
            <div class="col col-50">
                <div class="seaImgLayout">
                     <div class="iProList floorMain" ng-repeat="proLeftDatas in proDatas.proLeftInfos track by $index">
                        <img src="img/nopic.jpg" _lazysrc="{{proLeftDatas.proImg}}" ng-click="proLeftClick($index)"  onerror=this.src=errImg>
                        <h2 ng-bind="proLeftDatas.proPrice" class="proPrice margin-top-none margin-bottom-none"></h2>
                        <h2 class='proName_style1 margin-bottom-none margin-top-none' ng-bind="proLeftDatas.proName"></h2>
                    </div>
                </div>
               
            </div>

            <div class="col col-50">
                <div class="seaImgLayout">
                    <div class="iProList floorMain" ng-repeat="proRightDatas in proDatas.proRightInfos track by $index">
                        <img src="img/nopic.jpg" _lazysrc="{{proRightDatas.proImg}}" ng-click="proRightClick($index)" onerror=this.src=errImg>
                        <h2 ng-bind="proRightDatas.proPrice" class="proPrice margin-top-none margin-bottom-none"></h2>
                        <h2 class="proName_style1 margin-bottom-none margin-top-none" ng-bind="proRightDatas.proName"></h2>
                    </div>
                </div>
                
            </div>
        </div>

        <ion-infinite-scroll class="spinner spinner-ios" distance="0px" on-infinite="proResLoadMore()" ng-hide="loadingHide"></ion-infinite-scroll>
        <h2 class="noMore margin-top-none" ng-hide="noMoreHide">已经到底啦</h2>
    </ion-content>
</ion-view>
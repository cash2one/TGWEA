<ion-view title="支付方式">
    <ion-nav-buttons side="left">
        <button ng-click="back()" class="button button-clear button-light ion-ios-arrow-back"></button>
    </ion-nav-buttons>

    <ion-content>
        <div class="list list-inset iPayTypes">
            <ion-radio ng-value="payTypes_a.payName" ng-model="data.payType" ng-if="payTypes_a.payHide === false">
                <div id="payTypeIcon_a"></div>
                <div id="payTypeText" ng-bind="payTypes_a.payName"></div>
            </ion-radio>
            <ion-radio ng-value="payTypes_w.payName" ng-model="data.payType" ng-if="payTypes_w.payHide === false">
                <div id="payTypeIcon_w"></div>
                <div id="payTypeText" ng-bind="payTypes_w.payName"></div>
            </ion-radio>
        </div>

        <div id="payBtn">
            <button class="button button-block button-light" ng-click="settingPayType(data.payType)">去支付</button>
        </div>
      
    </ion-content>
</ion-view>
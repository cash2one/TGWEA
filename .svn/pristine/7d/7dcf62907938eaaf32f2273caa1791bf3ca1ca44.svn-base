	font-family:"方正兰亭黑简体";
}
.loginIcon{
	width:35px;
	height:35px;
	position: absolute;
	margin:10px 5px 0px 5px;
	font-family:"方正兰亭黑简体";
}
.promptText{
    display: none;
	font-family:"方正兰亭黑简体";
	font-size: 16px;
	color: #888888;
}
.myListView{
	width:98%;
	background: white;
	border-radius: 3px; 

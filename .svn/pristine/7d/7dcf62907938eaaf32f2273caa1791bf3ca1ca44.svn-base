@charset "UTF-8";
/* CSS Document */

h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6{
	font-family:"方正兰亭黑简体";
}
html{
	font-family:"方正兰亭黑简体";
    -webkit-user-select:none;
}

@font-face { 
  font-family: "方正兰亭黑简体"; 
  src:"../fonts/兰亭黑简.TTF";
}

.swiper-container {
  height: 200px;
}

.mpagination,.ipagination,
.apagination{
  position: absolute;
  z-index: 500;
  left: 10px;
  bottom: 10px;
  clear: both;
  font-family:"方正兰亭黑简体";
}

.swiper-pagination-switch {
  display: inline-block;
  width: 8px;
  height: 8px;
  border-radius: 8px;
  background: #555;
  margin-right: 5px;
  opacity: 0.8;
  border: 1px solid #fff;
  cursor: pointer;
  font-family:"方正兰亭黑简体";
}
.swiper-active-switch {
  background: #fff;
}

body{
	margin:0px ;
    font-family:"方正兰亭黑简体";
  	font-size: 13px;
  	line-height: 1.5em;
  	overflow-x:hidden;
    -webkit-user-select:none;
    -webkit-touch-callout: none;
}

footer{
	margin:0px 0px 0px 0px;
    padding:0px 0px 0px 0px;
    bottom:0px;
    position:fixed;
    width:100%;
    height: auto;
    z-index:1001;
	pointer-events:auto;
	font-family:"方正兰亭黑简体";
}

.content{
	min-height:100%;
	font-family:"方正兰亭黑简体";
}

.myAlertView{
	display: none;
	width: 100%;
	position: fixed;
	z-index: 20000;
	height: auto;
	text-align:center; 
	top: 60%;
	font-family:"方正兰亭黑简体";
}

.myAlertMessage{
	height: auto;
	width: 130px;
	background: #f25454;
	margin:0 auto;
	padding: 5px;
	font-size: 14px;
	font-weight:bold;
	color: white;
	border-radius:3px;
	box-shadow: 0 0 5px #605d5d;
	word-wrap:break-word;
	word-break:break-all;
	font-family:"方正兰亭黑简体";
}

.buttonClass-a a,.proSearch a,
.buttonClass-a a:link,.buttonClass-a a:visited,
.buttonClass-a a:hover,.buttonClass-a a:active{
	display:block;
	width:300px;
	height:48px;
	line-height:48px;
	border-radius:2px;
	color:#FFF;
	font-size: 17px;
	background:#35C28C;
	margin:5px 0px 10px 0px;
	font-family:"方正兰亭黑简体";
    text-decoration: none;
}
.proSearch a,
.proSearch a:link,.proSearch a:visited,
.proSearch a:active,.proSearch a:hover{
	width: 100px;
	height: 40px;
	line-height: 40px;
	margin: 10px 7px 7px 7px;
	text-align: center;
	font-family:"方正兰亭黑简体";
    text-decoration: none;
}
.buttonClass-b a,
.buttonClass-b a:link,.buttonClass-b a:visited,
.buttonClass-b a:hover,.buttonClass-b a:active{
	display:block;
	width:300px;
	height:44px;
	line-height:45px;
	border-radius:2px;
	color: #808080;
	font-size: 17px;
	margin: 0px 0px 10px 0px;
	background: #ffffff;
	border: 1px solid #cecdcd;
    text-decoration: none;
	box-shadow: 0px 1px 1px 0 rgba(0, 0, 0, 0.1);
	font-family:"方正兰亭黑简体";
}
.updateBtn img,
.updateBtn img:link,.updateBtn img:visited,
.updateBtn img:hover{
	font-family:"方正兰亭黑简体";
	background-size:17px 17px;
	color:#FFF;
    text-decoration: none;
}
.updateBtn img{
	display:block;
	width:20px;
	height:20px;
	margin: 3px 0px 0px 0px;
	border-radius:5px;
	font-family:"方正兰亭黑简体";
}
.updateBtn img:active{
	background:#34A277 url(../img/adminPro/updateBtnC.png)  no-repeat center;
	background-size:17px 17px;
	border:2px solid #FFF;
	color:#FFF;
	font-family:"方正兰亭黑简体";
}
.navigationLeftBtn{
	width:40px;
	position: absolute;
	left: -10px;
    text-align: left;
	font-family:"方正兰亭黑简体";
}
.navigationRightBtn{
	width: 40px;
	position: absolute;
	right:60px;
	text-align: left;
	font-family:"方正兰亭黑简体";
}

.navigationImg{
	width:5px;
	position: absolute;
	height: 25px;
	margin: 13px -5px 5px 0px;
	right:45px;
	border-right: 1px solid #989796;
}

.navigationMenuBtn{
	width: 40px;
	position: absolute;
	right:10px;
	text-align: left;
	/*margin-left:0px;*/
	font-family:"方正兰亭黑简体";
}
.navigationItem a{
	display: block;
	width:120%;
	height:44px;
	margin:0px 0px 0px 10px;
    padding-left: 13px;
	line-height:48px;
	font-size:15px;
	color:#555;
	background-color:transparent;
	text-shadow:none;
	font-family:"方正兰亭黑简体";
}
.navigationItem a:link,
.navigationItem a:visited,
.navigationItem a:hover{
    text-decoration: none;
	color:#555;
	font-family:"方正兰亭黑简体";
}
.navigationItem a:active{
	color: white;
	text-shadow:1px 0 1px #939393, 0 1px 1px #939393,
	0 -1px 5px white, -1px 0 5px white;
	font-family:"方正兰亭黑简体";
}
/*留言板*/
.messageView{
width: 100%;
height: 100%;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent{
height: 100%;
margin: 0;padding: .5em 1em;
overflow: hidden;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent img {
max-width: 100%;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li{
position: relative;
margin: 1em 0;
list-style: none; 
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li:after{
content:".";
display:block;
height:0;
clear:both;
visibility:hidden;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li .messageImg{
width: 55px;
height: 55px;
float: left;
margin-right: 1em;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li .messageImg img{
	-webkit-border-radius: 50em;
	-moz-border-radius: 50em;
	border-radius: 50em;
}
.messageView #messageContent li .messageBg{
float: left;
width: 76%;
max-width:76%;
min-height: 40px;
height: 100%;
font-size: 14px;
background-color: white;
padding: 1px 0px 1px 0px;
border-radius: .4em;
border: 1px solid #dfdcdc;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li .userName{
	display: block;
	line-height: 1em;
	position: relative;
	padding:.7em 1.2em .3em 1.2em;
	text-align: left;
	font-size: 17px;
	font-weight: bold;
	word-wrap: break-word;
	font-family:"方正兰亭黑简体";
}
.messageView #messageContent li .messageText{
color: #626161;
font-weight: bold;
display: block;
padding:.7em 1.3em .5em 1.3em;
line-height: 1.3em;
word-wrap: break-word;
text-align: left;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li .userName:after,
.messageView #messageContent li .userName:before{
border: solid rgba(0, 0, 0, 0);
content: " ";
height: 0;
width: 0;
top: 14px;
position: absolute;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li .userName:after{
right: 100%;	
border-right-color:#6c6c6c;
border-width: 8px;
margin-top: 0px;
font-family:"方正兰亭黑简体";
}
.messageView #messageContent li .messageDate{
	display: block;
	line-height: 1.3em;
	padding:0 1.9em .7em 1.9em;
	text-align: right;
	color: #a1a0a0;
	font-size: 13px;
	font-weight: bold;
	word-wrap: break-word;
	font-family:"方正兰亭黑简体";
}

.indexMenuBg{
	width: 100%;
	height: 40px;
	font-family:"方正兰亭黑简体";
}

.indexMenuIconBtn{
	position:absolute;
	width:60px;
	height:auto;
	pointer-events:auto;
	background-color: transparent;
	border:none;
	left: 50%;
	margin-left: -30px;
	*margin-left: -30px;
	_margin-left: -30px;
	background-attachment: fixed;
	font-family:"方正兰亭黑简体";
}
.indexMenuIconBtn img{
	width:40px;
	height: 40px;
	margin-right: 50%;
	margin-left: 10px;
	*margin-right: 50%;
	*margin-left: 10px;
	_margin-right: 50%;
	_margin-left: 10px;
	font-family:"方正兰亭黑简体";
}

.scrollerBg{
	margin:2px auto;
	height:auto;
	padding:0px;
	font-family:"方正兰亭黑简体";
}
#contactInfo{
	padding: 0px 10px;
	font-family:"方正兰亭黑简体";
}
#contactInfo p{
	margin: 0 0 2px;
	font-family:"方正兰亭黑简体";
}
#contenttext,#contenttexts{
	padding:10px 0px 0px 0px;
	text-align:left;
	position: fixed;
/*	overflow:hidden;*/
	font-family:"方正兰亭黑简体";
}
#contenttext,#contenttexts{
	overflow-y:auto;
	font-family:"方正兰亭黑简体";
}
#contenttext code p{
	padding: 0px 10px;
	color: black;
	text-indent:2em;
	white-space:normal;
	word-wrap:break-word;
	word-break:break-all;
	font-family:"方正兰亭黑简体";
}
#about .content,#contact .content{
	min-height: 0px;
	font-family:"方正兰亭黑简体";
}

.classindexmenu{
	height:auto;
	width:100%;
	pointer-events:auto;
	font-family:"方正兰亭黑简体";
}

/** 产品展示图片样式 **/
.keepOut{
	margin-top: -2px; 
	width: 100%;
	height: 100%;
	background: gray;
	position: fixed;
	left: 0px;
	z-index: 900;
	opacity: 0.3;
	font-family:"方正兰亭黑简体";
}
.proshowview1{
	margin:0px 0px 2px 0px; 
	padding:0px 0px;
	float:left;
	font-family:"方正兰亭黑简体";
}
.proshowview2,.proshowview3,
.proshowview4{
	margin:0px 0px 2px 2px;
	padding:0px 0px;
	float:left;
	font-family:"方正兰亭黑简体";
}
.noMore,.noMessage,
.noOrder,.anoMorePro,
.noMyOrder,.fproMore,
.proListMore,.noMessage,
.noMyWaitingOrder,.searUserMore,
.superProMore{
	display:none;
	height: 40px;
	line-height: 40px;
	color:#565656;
	font-family:"方正兰亭黑简体";
}


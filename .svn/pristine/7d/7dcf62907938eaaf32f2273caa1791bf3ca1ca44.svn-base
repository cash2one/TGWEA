	background-color:rgba(255,255,255,0.8);
	font-family:"方正兰亭黑简体";
}
.proTitlePriceRight{
	color:#2a2929;
	padding:4px 4px 4px 4px;
	top: 0px;
	left: 0px;
	text-align:right;
	position: absolute;
	white-space:normal;
	word-wrap:break-word;
	word-break:break-all;
	text-shadow: none;
	background-color:rgba(255,255,255,0.8);
	font-family:"方正兰亭黑简体";
}
.conPrompt{
	overflow-y:hidden;
	overflow-x:hidden;
	background: #f56c6c;
	width: 100%;
	height:50px;
	z-index: 999999;
	text-align: left;
	position:fixed;
	font-family:"方正兰亭黑简体";
}
.conPrompt span{
	margin-left:10px;
	line-height: 50px;
	font-size: 20px;
	font-family:"方正兰亭黑简体";
}
.productcontent{
	width:100%;
	margin:2px 0px 0px 0px;
	padding:0px 0px;
	line-height:0px;
	overflow:hidden;
	clear: both;
	font-family:"方正兰亭黑简体";
}
.proText{
	position:relative;
	text-align:left;
	margin:10px 10px;
	z-index:300;
	font-family:"方正兰亭黑简体";
}

.info, .contact{
	width:50%;
	height:100%; 
	float:left;
	margin:0;
	font-size: 14px;
	font-weight: bolder;
	font-family:"方正兰亭黑简体";
}
.contact a span .ui-btn-text, 
.info a span .ui-btn-text{
	font-size:15px;
	font-family:"方正兰亭黑简体";
}
.segmentedView{
	margin:0 auto; 
	width:300px;
	height:auto;
	padding:0px;
	height:44px;
	font-family:"方正兰亭黑简体";
}
.segmentedBg{
	margin:0px 0px 0px 0px;
	padding:0px;
	height:44px;
	position:fixed;
	width:300px;
	z-index:500;
	border-radius: 2px;
	border: 1px solid #e9e7e7;
	font-family:"方正兰亭黑简体";
}
.segmenteRightBtnDefault,
.segmenteLeftBtnDefault,
.segmentItemBtnDefalut{
	display: block;
	line-height:245%;
	width:100%;
	height:100%;
	background: white;
	font-size: 16px;
	color: #808080;
	font-family:"方正兰亭黑简体";
}
.segmenteRightBtnDefault,
.segmenteRightBtnClick{
	border-top-right-radius: 2px;
	border-bottom-right-radius: 2px;
	font-family:"方正兰亭黑简体";
}
.segmenteLeftBtnClick,
.segmenteRightBtnClick,
.segmentItemBtnClick{
	display: block;
	line-height:245%;
	width:100%;
	height:100%;
	background: #35C28C;
	font-size: 16px;
	color: white;
	font-family:"方正兰亭黑简体";
}
.segmenteLeftBtnClick,
.segmenteLeftBtnDefault{
	border-top-left-radius: 2px;
	border-bottom-left-radius: 2px;
	font-family:"方正兰亭黑简体";
}
.segmenteLeftBtnClick a,
.segmenteRightBtnClick a,
.segmentItemBtnClick a,
.segmenteLeftBtnClick a:hover,
.segmenteLeftBtnClick a:active,
.segmenteLeftBtnClick a:visited,
.segmenteLeftBtnClick a:link,
.segmenteRightBtnClick a:hover,
.segmenteRightBtnClick a:active,
.segmenteRightBtnClick a:visited,
.segmenteRightBtnClick a:link,
.segmentItemBtnClick a:hover,
.segmentItemBtnClick a:active,
.segmentItemBtnClick a:visited,
.segmentItemBtnClick a:link{
	display: block;
	width: 100%;
	height: 100%;
	background: #35C28C;
	color: white;
    text-decoration: none;
	font-family:"方正兰亭黑简体";
}
.segmenteRightBtnDefault a,
.segmenteLeftBtnDefault a,
.segmentItemBtnDefalut a,
.segmenteRightBtnDefault a:active,
.segmenteLeftBtnDefault a:active,
.segmentItemBtnDefalut a:active,
.segmenteRightBtnDefault a:visited,
.segmenteRightBtnDefault a:link,
.segmenteRightBtnDefault a:hover,
.segmenteLeftBtnDefault a:visited,
.segmenteLeftBtnDefault a:link,
.segmenteLeftBtnDefault a:hover,
.segmentItemBtnDefalut a:visited,
.segmentItemBtnDefalut a:link,
.segmentItemBtnDefalut a:hover{
	display: block;
	background: white;
	color: #808080;
	width: 100%;
	height: 100%;
    text-decoration: none;
	font-family:"方正兰亭黑简体";
}

.ordercontent{
	width:95%;
	font-family:"方正兰亭黑简体";
}
.buttonView{

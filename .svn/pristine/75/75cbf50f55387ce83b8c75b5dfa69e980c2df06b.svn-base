package com.etaoguan.wea.client.vo;

public class PageNavInfo {

	int totalRecNum;
	
	int currentPage;
	
	int totalPages;
	
	boolean havePrePage;
	
	boolean haveNextPage;


	public int getCurrentPage() {
		return currentPage;
	}

	public void setCurrentPage(int currentPage) {
		this.currentPage = currentPage;
	}

	public boolean isHavePrePage() {
		return havePrePage;
	}

	public void setHavePrePage(boolean havePrePage) {
		this.havePrePage = havePrePage;
	}

	public boolean isHaveNextPage() {
		return haveNextPage;
	}

	public void setHaveNextPage(boolean haveNextPage) {
		this.haveNextPage = haveNextPage;
	}

	public int getTotalPages() {
		return totalPages;
	}

	public void setTotalPages(int totalPages) {
		this.totalPages = totalPages;
	}

	public int getTotalRecNum() {
		return totalRecNum;
	}

	public void setTotalRecNum(int totalRecNum) {
		this.totalRecNum = totalRecNum;
	}
}
